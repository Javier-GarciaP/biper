{
  "name": "react-biper",
  "version": "0.1.1",
  "description": "Una librería moderna y minimalista de notificaciones para React con animaciones elegantes y soporte completo para TypeScript",
  "keywords": [
    "react",
    "notifications",
    "toast",
    "alerts",
    "snackbar",
    "notificaciones",
    "react-component",
    "typescript"
  ],
  "author": "José Javier García",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.umd.js",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js"
    },
    "./style.css": "./dist/react-biper.css"
  },
  "scripts": {
    "dev": "npm run dev --prefix website",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "npm run preview --prefix website",
    "prepublishOnly": "npm run build",
    "prepare": "npm run build",
    "pack": "npm pack"
  },
  "dependencies": {},
  "peerDependencies": {
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@tailwindcss/vite": "^4.1.18",
    "@types/node": "^24.10.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react-swc": "^4.2.2",
    "autoprefixer": "^10.4.23",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "lucide-react": "^0.562.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.18",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.4",
    "vite-plugin-css-injected-by-js": "^3.5.2",
    "vite-plugin-dts": "^4.5.4"
  }
}